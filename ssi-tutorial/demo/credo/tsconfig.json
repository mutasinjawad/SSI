{
    "compilerOptions": {
        "target": "ES6",
        "baseUrl": "./",
        "esModuleInterop": true,
        "moduleResolution": "node",
        "paths": {},
        "sourceMap": true,
        "sourceRoot": "/",
        "allowUnreachableCode": false,
        "noImplicitAny": true,
        "strictNullChecks": true
    },
    "include": [
        "./**/*.ts"
    ],
    "exclude": [
        "node_modules/**/*"
    ]
}
